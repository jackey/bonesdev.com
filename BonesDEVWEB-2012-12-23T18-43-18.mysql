INSERT INTO `locales_source` VALUES ('3185','','default','For more information see W3C''s <a href="@html-specifications">HTML Specifications</a> or use your favorite search engine to find other sites that explain HTML.','','7.13-dev'),('3186','','default','Most unusual characters can be directly entered without any problems.','','7.13-dev'),('3187','','default','No HTML tags allowed.','','7.13-dev'),('3188','','default','Filters content in preparation for display.','','7.13-dev'),('3189','','default','By !author @time ago','','none'),('3190','','default','The forum %term and all sub-forums have been deleted.','','none'),('3191','','default','The number of replies a topic must have to be considered "hot".','','none'),('3192','','default','No containers or forums available. <a href="@container">Add container</a> or <a href="@forum">Add forum</a>.','','none'),('3193','','default','forum: deleted %term and all its sub-forums.','','none'),('3194','','default','The Forum module lets you create threaded discussion forums with functionality similar to other message board systems. Forums are useful because they allow community members to discuss topics with one another while ensuring those conversations are archived for later reference. In a forum, users post topics and threads in nested hierarchies, allowing discussions to be categorized and grouped. The forum hierarchy consists of:','','none'),('3195','','default','Optional containers (for example, <em>Support</em>), which can hold:','','none'),('3196','','default','Forums (for example, <em>Installing Drupal</em>), which can hold:','','none'),('3197','','default','Setting up forum structure','','none'),('3198','','default','Starting a discussion','','none'),('3199','','default','The <a href="@create-topic">Forum topic</a> link on the <a href="@content-add">Add new content</a> page creates the first post of a new threaded discussion, or thread.','','none'),('3200','','default','Moving forum topics','','none'),('3201','','de