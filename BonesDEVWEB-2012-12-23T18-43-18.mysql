INSERT INTO `locales_source` VALUES ('2817','','default','The file''s name exceeds the 240 characters limit. Please rename the file and try again.','','none'),('2818','','default','%path is a directory and cannot be removed using file_unmanaged_delete().','','none'),('2819','','default','The file %path is not of a recognized type so it was not deleted.','','none'),('2820','','default','The file permissions could not be set on %uri.','','none'),('2821','','default','Completed @current of @total.','','none'),('2822','','default','Failed to run all tasks against the database server. The task %task wasn''t found.','','none'),('2823','','default','Failed to modify %settings. Verify the file permissions.','','none'),('2824','','default','Failed to open %settings. Verify the file permissions.','','none'),('2825','','default','Required modules','','none'),('2826','','default','Required modules not found.','','none'),('2827','','default','The following modules are required but were not found. Move them into the appropriate modules subdirectory, such as <em>sites/all/modules</em>. Missing modules: !modules','','none'),('2828','','default','system','','7.13-dev'),('2829','','default','%module module uninstalled.','','none'),('2830','','default','Saint Barthélemy','','none'),('2831','','default','Saint Martin (French part)','','none'),('2832','','default','The translation file %filepath contains an error: the plural formula could not be parsed.','','none'),('2833','','default','No strings available.','','7.13-dev'),('2834','','default','@count disallowed HTML string(s) in %file','','none'),('2835','','default','JavaScript translation file %file.js was lost.','','none'),('2836','','default','Unable to send e-mail. Contact the site administrator if the problem persists.','','none'),('2837','','default','Class %class does not implement interface %interface','','none'),('2838','','default','Operating in maintenance mode. <a href="@url">Go online.</a>','','none'),('2839','','default