INSERT INTO `locales_source` VALUES ('5306','/admin/config/content/wysiwyg/profile/full_html/edit','default','Cleanup and output','','7.13-dev'),('5307','/admin/config/content/wysiwyg/profile/full_html/edit','default','Verify HTML','','7.13-dev'),('5308','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, potentially malicious code like <code>&lt;HEAD&gt;</code> tags will be removed from HTML contents.','','7.13-dev'),('5309','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, the editor will insert TAB characters on tab and preserve other whitespace characters just like a PRE element in HTML does.','','7.13-dev'),('5310','/admin/config/content/wysiwyg/profile/full_html/edit','default','Convert &lt;font&gt; tags to styles','','7.13-dev'),('5311','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, HTML tags declaring the font size, font family, font color and font background color will be replaced by inline CSS styles.','','7.13-dev'),('5312','/admin/config/content/wysiwyg/profile/full_html/edit','default','Remove linebreaks','','7.13-dev'),('5313','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, the editor will remove most linebreaks from contents. Disabling this option could avoid conflicts with other input filters.','','7.13-dev'),('5314','/admin/config/content/wysiwyg/profile/full_html/edit','default','Apply source formatting','','7.13-dev'),('5315','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, the editor will re-format the HTML source code. Disabling this option could avoid conflicts with other input filters.','','7.13-dev'),('5316','/admin/config/content/wysiwyg/profile/full_html/edit','default','Force cleanup on standard paste','','7.13-dev'),('5317','/admin/config/content/wysiwyg/profile/full_html/edit','default','If enabled, the default paste function (CTRL-V or SHIFT-INS) behaves like the "paste from word" plugin functio