INSERT INTO `locales_source` VALUES ('2842','','default','Subscribe to @feed-title','','7.13-dev'),('2843','','default','Theme key "@key" not found.','','none'),('2844','','default','Unable to determine the type of the source directory.','','none'),('2845','','default','Cannot determine the type of project.','','none'),('2846','','default','Fatal error in update, cowardly refusing to wipe out the install directory.','','none'),('2847','','default','File Transfer failed, reason: !reason','','none'),('2848','','default','Unable to create %directory due to the following: %reason','','none'),('2849','','default','Drupal could not be correctly setup with the existing database. Revise any errors.','','none'),('2850','','default','No feeds available. <a href="@link">Add feed</a>.','','none'),('2851','','default','No categories available. <a href="@link">Add category</a>.','','none'),('2852','','default','The length of time between feed updates. Requires a correctly configured <a href="@cron">cron maintenance task</a>.','','none'),('2853','','default','News items in block','','none'),('2854','','default','Drupal can make a block with the most recent news items of this feed. You can <a href="@block-admin">configure blocks</a> to be displayed in the sidebar of your page. This setting lets you configure the number of news items to show in this feed''s block. If you choose ''0'' this feed''s block will be disabled.','','none'),('2855','','default','The URL %url is invalid. Enter a fully-qualified URL, such as http://www.example.com/feed.xml.','','none'),('2856','','default','A feed named %feed already exists. Enter a unique title.','','none'),('2857','','default','A feed with this URL %url already exists. Enter a unique URL.','','none'),('2858','','default','Upload an OPML file containing a list of feeds to be imported.','','none'),('2859','','default','OPML Remote URL','','none'),('2860','','default','Enter the URL of an OPML file. This file will be downloaded and processed only once on submission of the form.','','none'),('2861','','default','Drupal can make a block with the most recent news items of a feed. You can <a href="@block-admin">configure blocks</a> to be displayed in the sidebar of your page. This setting lets you configure the number of news items to show in a feed''s block. If you choose ''0'' these feeds'' blocks will be disabled.','','none');
INSERT INTO `locales_source` VALUES ('2862','','default','You must <em>either</em> upload a file or enter a URL.','','none'),('2863','','default','This URL is not valid.','','none'),('2864','','default','No new feed has been added.','','none'),('2865','','default','The URL %url is invalid.','','none'),('2866','','default','A feed named %title already exists.','','none'),('2867','','default','A feed with the URL %url already exists.','','none'),('2868','','default','Fetchers download data from an external source. Choose a fetcher suitable for the external source you would like to download from.','','none'),('2869','','default','Parsers transform downloaded data into standard structures. Choose a parser suitable for the type of feeds you would like to aggregate.','','none'),('2870','','default','Processors act on parsed feed data, for example they store feed items. Choose the processors suitable for your task.','','none'),('2871','','default','For most aggregation tasks, the default settings are fine.','','none'),('2872','','default','A category named %category already exists. Enter a unique title.','','none'),('2873','','default','Default fetcher','','none'),('2874','','default','Downloads data from a URL using Drupal''s HTTP request handler.','','none'),('2875','','default','Default parser','','none'),('2876','','default','Parses RSS, Atom and RDF feeds.','','none'),('2877','','default','Default processor','','none'),('2878','','default','Creates lightweight records from feed items.','','none'),('2879','','default','Default processor settings','','none'),('2880','','default','Number of items shown in listing pages','','none'),('2881','','default','Requires a correctly configured <a href="@cron">cron maintenance task</a>.','','none'),('2882','','default','Select categories using','','none'),('2883','','default','For a small number of categories, checkboxes are easier to use, while a multiple selector works well with large numbers of categories.','','none'),('2884','','default','Length of trimmed description','','none'),('2885','','default','The maximum number of characters used in the trimmed version of content.','','none');
INSERT INTO `locales_source` VALUES ('2886','','default','The Aggregator module is an on-site syndicator and news reader that gathers and displays fresh content from RSS-, RDF-, and Atom-based feeds made available across the web. Thousands of sites (particularly news sites and blogs) publish their latest headlines in feeds, using a number of standardized XML-based formats. For more information, see the online handbook entry for <a href="@aggregator-module">Aggregator module</a>.','','none'),('2887','','default','Viewing feeds','','none'),('2888','','default','Feeds contain published content, and may be grouped in categories, generally by topic. Users view feed content in the <a href="@aggregator">main aggregator display</a>, or by <a href="@aggregator-sources">their source</a> (usually via an RSS feed reader). The most recent content in a feed or category can be displayed as a block through the <a href="@admin-block">Blocks administration page</a>.','','none'),('2889','','default','Adding, editing, and deleting feeds','','none'),('2890','','default','Administrators can add, edit, and delete feeds, and choose how often to check each feed for newly updated items on the <a href="@feededit">Feed aggregator administration page</a>.','','none'),('2891','','default','OPML integration','','none'),('2892','','default','A <a href="@aggregator-opml">machine-readable OPML file</a> of all feeds is available. OPML is an XML-based file format used to share outline-structured information such as a list of RSS feeds. Feeds can also be <a href="@import-opml">imported via an OPML file</a>.','','none'),('2893','','default','Configuring cron','','none'),('2894','','default','A correctly configured <a href="@cron">cron maintenance task</a> is required to update feeds automatically.','','none'),('2895','','default','<acronym title="Outline Processor Markup Language">OPML</acronym> is an XML format used to exchange multiple feeds between aggregators. A single OPML document may contain a collection of many feeds. Drupal can parse such a file and import all feeds at once, saving you the effort of adding them manually. You may either upload a local file from your computer or enter a URL where Drupal can download it.','','none');
INSERT INTO `locales_source` VALUES ('2896','','default','Administer news feeds','','none'),('2897','','default','View news feeds','','none'),('2898','','default','Configure the behavior of the feed aggregator, including when to discard feed items and how to present feed items and categories.','','none'),('2899','','default','Override the default title for the block. Use <em>!placeholder</em> to display no title, or leave blank to use the default block title.','','none'),('2900','','default','Specify in which themes and regions this block is displayed.','','7.13-dev'),('2901','','default','All pages except those listed','','7.13-dev'),('2902','','default','Only the listed pages','','7.13-dev'),('2903','','default','Specify pages by using their paths. Enter one path per line. The ''*'' character is a wildcard. Example paths are %blog for the blog page and %blog-wildcard for every personal blog. %front is the front page.','','7.13-dev'),('2904','','default','Pages on which this PHP code returns <code>TRUE</code> (experts only)','','none'),('2905','','default','Pages or PHP code','','none'),('2906','','default','If the PHP option is chosen, enter PHP code between %php. Note that executing incorrect PHP code can break your Drupal site.','','none'),('2907','','default','Customizable per user','','7.13-dev'),('2908','; modules/block/block.js','default','Not customizable','','7.13-dev'),('2909','','default','Customizable, visible by default','','7.13-dev'),('2910','','default','Customizable, hidden by default','','7.13-dev'),('2911','','default','Ensure that each block description is unique.','','none'),('2912','','default','The Block module allows you to create boxes of content, which are rendered into an area, or region, of one or more pages of a website. The core Seven administration theme, for example, implements the regions "Content", "Help", "Dashboard main", and "Dashboard sidebar", and a block may appear in any one of these regions. The <a href="@blocks">Blocks administration page</a> provides a drag-and-drop interface for assigning a block to a region, and for controlling the order of blocks within regions. For more information, see the online handbook entry for <a href="@block">Block module</a>.','','7.13-dev');
INSERT INTO `locales_source` VALUES ('2913','','default','Positioning content','','7.13-dev'),('2914','','default','When working with blocks, remember that all themes do <em>not</em> implement the same regions, or display regions in the same way. Blocks are positioned on a per-theme basis. Users with the <em>Administer blocks</em> permission can disable blocks. Disabled blocks are listed on the <a href="@blocks">Blocks administration page</a>, but are not displayed in any region.','','7.13-dev'),('2915','','default','Controlling visibility','','7.13-dev'),('2916','','default','Blocks can be configured to be visible only on certain pages, only to users of certain roles, or only on pages displaying certain <a href="@content-type">content types</a>. Administrators can also allow specific blocks to be enabled or disabled by users when they edit their <a href="@user">My account</a> page. Some dynamic blocks, such as those generated by modules, will be displayed only on certain pages.','','7.13-dev'),('2917','','default','Creating custom blocks','','7.13-dev'),('2918','','default','Users with the <em>Administer blocks</em> permission can <a href="@block-add">add custom blocks</a>, which are then listed on the <a href="@blocks">Blocks administration page</a>. Once created, custom blocks behave just like default and module-generated blocks.','','7.13-dev'),('2919','','default','This page provides a drag-and-drop interface for assigning a block to a region, and for controlling the order of blocks within regions. Since not all themes implement the same regions, or display regions in the same way, blocks are positioned on a per-theme basis. Remember that your changes will not be saved until you click the <em>Save blocks</em> button at the bottom of the page. Click the <em>configure</em> link next to each block to configure its specific title and visibility settings.','','7.13-dev'),('2920','','default','Demonstrate block regions (@theme)','','7.13-dev'),('2921','','default','Administer blocks','','7.13-dev'),('2922','','default','A brief description of your block. Used on the <a href="@overview">Blocks administration page</a>.','','7.13-dev');
INSERT INTO `locales_source` VALUES ('2923','','default','Personalize blocks','','none'),('2924','','default','Blocks consist of content or information that complements the main content of the page. Enable or disable optional blocks using the checkboxes below.','','none'),('2925','','default','Cache blocks','','none'),('2926','','default','Block caching is inactive because you have enabled modules defining content access restrictions.','','none'),('2927','','default','Controls the visual building blocks a page is constructed with. Blocks are boxes of content rendered into an area, or region, of a web page.','','7.13-dev'),('2928','; modules/block/block.js','default','Restricted to certain pages','','none'),('2929','; modules/block/block.js','default','The block cannot be placed in this region.','','none'),('2930','','default','Use for multi-user blogs. Every user gets a personal blog.','','none'),('2931','','default','Single-user blogs','','none'),('2932','','default','Each user''s blog entries are automatically displayed with a link to the user''s main blog page. You can create as many single-user blogs as you have site users with permission to create blog content.','','none'),('2933','','default','Multi-user blogs','','none'),('2934','','default','Blog entries from each single-user blog are also aggregated into one central multi-user blog, which displays the blog content of all users in a single listing.','','none'),('2935','','default','There is an optional <em>Blogs</em> menu item added to the Navigation menu, which displays all blogs available on your site, and a <em>My blog</em> item displaying the current user''s blog entries.','','none'),('2936','','default','The Blog module also creates a default <em>Recent blog posts</em> block that may be enabled at the <a href="@blocks">blocks administration page</a>.','','none'),('2937','','default','Create new blog entry','','none'),('2938','','default','Enables multi-user blogs.','','7.13-dev'),('2939','','default','No books available.','','none'),('2940','','default','Content types allowed in book outlines','','none');
INSERT INTO `locales_source` VALUES ('2941','','default','Users with the %outline-perm permission can add all content types.','','none'),('2942','','default','Administer book outlines','','none'),('2943','','default','The Book module is used for creating structured, multi-page content, such as site resource guides, manuals, and wikis. It allows you to create content that has chapters, sections, subsections, or any similarly-tiered structure. For more information, see the online handbook entry for <a href="@book">Book module</a>.','','none'),('2944','','default','Adding and managing book content','','none'),('2945','','default','You can assign separate permissions for <em>creating</em>, <em>editing</em>, and <em>deleting</em> book content, as well as <em>adding content to books</em>, and <em>creating new books</em>. Users with the <em>Administer book outlines</em> permission can add <em>any</em> type of content to a book by selecting the appropriate book outline while editing the content. They can also view a list of all books, and edit and rearrange section titles on the <a href="@admin-book">Book administration page</a>.','','none'),('2946','','default','Book pages have a default book-specific navigation block. This navigation block contains links that lead to the previous and next pages in the book, and to the level above the current page in the book''s structure. This block can be enabled on the <a href=''@admin-block''>Blocks administration page</a>. For book pages to show up in the book navigation, they must be added to a book outline.','','none'),('2947','','default','Books can be created collaboratively, as they allow users with appropriate permissions to add pages into existing books, and add those pages to a custom table of contents menu.','','none'),('2948','','default','Printing books','','none'),('2949','','default','Users with the <em>View printer-friendly books</em> permission can select the <em>printer-friendly version</em> link visible at the bottom of a book page''s content to generate a printer-friendly display of the page and all of its subsections.','','none');
INSERT INTO `locales_source` VALUES ('2950','','default','The book module offers a means to organize a collection of related content pages, collectively known as a book. When viewed, this content automatically displays links to adjacent book pages, providing a simple navigation system for creating and reviewing structured content.','','none'),('2951','','default','The outline feature allows you to include pages in the <a href="@book">Book hierarchy</a>, as well as move them within the hierarchy or to <a href="@book-admin">reorder an entire book</a>.','','none'),('2952','','default','Create new books','','none'),('2953','','default','Add content and child pages to books','','none'),('2954','','default','View printer-friendly books','','none'),('2955','','default','View a book page and all of its sub-pages as a single document for ease of printing. Can be performance heavy.','','none'),('2956','','default','<em>Books</em> have a built-in hierarchical navigation. Use for handbooks or tutorials.','','none'),('2957','','default','Allows users to create and organize related content in an outline.','','7.13-dev'),('2958','','default','The Color module allows users with the <em>Administer site configuration</em> permission to quickly and easily change the color scheme of themes that have been built to be compatible with it. For more information, see the online handbook entry for <a href="@color">Color module</a>.','','7.13-dev'),('2959','','default','Changing colors','','7.13-dev'),('2960','','default','Using the Color module allows you to easily change the color of links, backgrounds, text, and other theme elements. To change the color settings for a compatible theme, select the <em>Settings</em> link for your theme on the <a href=''@configure''>Themes administration page</a>. If you don''t see a color picker on that page, then your theme is not compatible with the color module. If you are sure that the theme does indeed support the color module, but the color picker does not appear, then <a href=''@troubleshoot''>follow these troubleshooting procedures</a>.','','7.13-dev');
INSERT INTO `locales_source` VALUES ('2961','','default','The Color module saves a modified copy of the theme''s specified stylesheets in the files directory. This means that if you make any manual changes to your theme''s stylesheet, <em>you must save your color settings again, even if they haven''t changed</em>. This step is required because the module stylesheets (in the files directory) need to be recreated to include your changes.','','7.13-dev'),('2962','','default','The GD library for PHP is enabled, but was compiled without PNG support. Check the <a href="@url">PHP image documentation</a> for information on how to correct this.','','none'),('2963','','default','The GD library for PHP is missing or outdated. Check the <a href="@url">PHP image documentation</a> for information on how to correct this.','','none'),('2964','','default','Allows administrators to change the color scheme of compatible themes.','','7.13-dev'),('2965','','default','Select one or more comments to perform the update on.','','none'),('2966','','default','There do not appear to be any comments to delete, or your selected comment was deleted by another administrator.','','none'),('2967','','default','Deleted @count comments.','','none'),('2968','','default','Deleted comment @cid and its replies.','','none'),('2969','','default','Comment approved.','','none'),('2970','','default','Tokens for comments posted on the site.','','7.13-dev'),('2971','','default','The Comment module allows users to comment on site content, set commenting defaults and permissions, and moderate comments. For more information, see the online handbook entry for <a href="@comment">Comment module</a>.','','7.13-dev'),('2972','','default','Default and custom settings','','7.13-dev'),('2973','','default','Each <a href=''@content-type''>content type</a> can have its own default comment settings configured as: <em>Open</em> to allow new comments, <em>Hidden</em> to hide existing comments and prevent new comments, or <em>Closed</em> to view existing comments, but prevent new comments. These defaults will apply to all new content created (changes to the settings on existing content must be done manually). Other comment settings can also be customized per content type, and can be overridden for any given item of content. When a comment has no replies, it remains editable by its author, as long as the author has a user account and is logged in.','','7.13-dev');
INSERT INTO `locales_source` VALUES ('2974','','default','Full comment','','7.13-dev'),('2975','','default','Unapproved comments (@count)','','7.13-dev'),('2976','','default','Administer comments and comment settings','','7.13-dev'),('2977','','default','Edit own comments','','7.13-dev'),('2978','','default','Number of recent comments','','none'),('2979','','default','Threading','','none'),('2980','','default','Show comment replies in a threaded list.','','none'),('2981','','default','Default comment setting for new content','','none'),('2982','','default','Allow comment title','','none'),('2983','','default','Show reply form on the same page as comments','','none'),('2984','','default','Users with the "Post comments" permission can post comments.','','7.13-dev'),('2985','','default','Users cannot post comments, but existing comments will be displayed.','','7.13-dev'),('2986','','default','Comments are hidden from view.','','7.13-dev'),('2987','','default','Users cannot post comments.','','7.13-dev'),('2988','','default','Your comment has been queued for review by site administrators and will be published after approval.','','none'),('2989','','default','Your comment has been posted.','','none'),('2990','','default','<a href="@login">Log in</a> or <a href="@register">register</a> to post comments','','7.13-dev'),('2991','','default','<a href="@login">Log in</a> to post comments','','none'),('2992','','default','Flat list','','none'),('2993','','default','Threaded list','','none'),('2994','','default','Save comment','','7.13-dev'),('2995','','default','The comment will be unpublished if it contains any of the phrases above. Use a case-sensitive, comma-separated list of phrases. Example: funny, bungee jumping, "Company, Inc."','','none'),('2996','','default','Published comment %subject.','','none'),('2997','','default','Saved comment %title','','none'),('2998','','default','List and edit site comments and the comment approval queue.','','7.13-dev'),('2999','','default','Unapproved comments','','none'),('3000','','default','Example: ''webmaster@example.com'' or ''sales@example.com,support@example.com'' . To specify multiple recipients, separate each e-mail address with a comma.','','none');
INSERT INTO `locales_source` VALUES ('3001','','default','Category %category has been saved.','','none'),('3002','','default','You cannot send more than %limit messages in @interval. Try again later.','','none'),('3003','','default','Contact @username','','none'),('3004','','default','%sender-name (@sender-from) sent an e-mail regarding %category.','','none'),('3005','','default','%sender-name (@sender-from) sent %recipient-name an e-mail.','','none'),('3006','','default','The Contact module allows visitors to contact site administrators and other users. Users specify a subject, write their message, and can have a copy of their message sent to their own e-mail address. For more information, see the online handbook entry for <a href="@contact">Contact module</a>.','','none'),('3007','','default','User contact forms','','none'),('3008','','default','Site users can be contacted with a user contact form that keeps their e-mail address private. Users may enable or disable their personal contact forms by editing their <em>My account</em> page. If enabled, a <em>Contact</em> tab leads to a personal contact form displayed on their user profile. Site administrators are still able to use the contact form, even if has been disabled. The <em>Contact</em> tab is not shown when you view your own profile.','','none'),('3009','','default','Site-wide contact forms','','none'),('3010','','default','The <a href="@contact">Contact page</a> provides a simple form for users with the <em>Use the site-wide contact form</em> permission to send comments, feedback, or other requests. You can create categories for directing the contact form messages to a set of defined recipients. Common categories for a business site, for example, might include "Website feedback" (messages are forwarded to website administrators) and "Product information" (messages are forwarded to members of the sales department). E-mail addresses defined within a category are not displayed publicly.','','none'),('3011','','default','When the site-wide contact form is enabled, a link in the main <em>Navigation</em> menu is created, but the link is disabled by default. This menu link can be enabled on the <a href=''@menu''>Menus administration page</a>.','','none');
INSERT INTO `locales_source` VALUES ('3012','','default','Customization','','none'),('3013','','default','If you would like additional text to appear on the site-wide or personal contact page, use a block. You can create and edit blocks on the <a href="@blocks">Blocks administration page</a>.','','none'),('3014','','default','Add one or more categories on this page to set up your site-wide <a href="@form">contact form</a>.','','none'),('3015','','default','A <em>Contact</em> menu item (disabled by default) is added to the Navigation menu, which you can modify on the <a href="@menu-settings">Menus administration page</a>.','','none'),('3016','','default','If you would like additional text to appear on the site-wide contact page, use a block. You can create and edit blocks on the <a href="@blocks">Blocks administration page</a>.','','none'),('3017','','default','Administer contact forms and contact form settings','','none'),('3018','','default','Use the site-wide contact form','','none'),('3019','','default','Use users'' personal contact forms','','none'),('3020','','default','!sender-name (!sender-url) sent a message using the contact form at !form-url.','','none'),('3021','','default','[!site-name] !subject','','none'),('3022','','default','!sender-name (!sender-url) has sent you a message via your contact form (!form-url) at !site-name.','','none'),('3023','','default','If you don''t want to receive such e-mails, you can change your settings at !recipient-edit-url.','','none'),('3024','','default','Allow other users to contact you via a <a href="@url">personal contact form</a> which keeps your e-mail address hidden. Note that some privileged users such as site administrators are still able to contact you even if you choose to disable this feature.','','none'),('3025','','default','Enable the personal contact form by default for new users.','','none'),('3026','','default','Changing this setting will not affect existing users.','','none'),('3027','','default','The Contextual links module displays links related to regions of pages on your site to users with <em>access contextual links</em> permission. For more information, see the online handbook entry for <a href="@contextual">Contextual links module</a>.','','7.13-dev');
INSERT INTO `locales_source` VALUES ('3028','','default','Displaying contextual links','','7.13-dev'),('3029','','default','Contextual links are supplied by modules, to give you quick access to tasks associated with regions of pages on your site. For instance, if you have a custom menu block displayed in a sidebar of your site, the Blocks and Menus modules will supply links to configure the block and edit the menu. The Contextual links module collects these links into a list for display by your theme, and also adds JavaScript code to the page to hide the links initially, and display them when your mouse hovers over the block.','','7.13-dev'),('3030','','default','Use contextual links','','7.13-dev'),('3031','','default','Use contextual links to perform actions related to elements on a page.','','7.13-dev'),('3032','','default','Contextual links','','none'),('3033','','default','Provides contextual links to perform actions related to elements on a page.','','7.13-dev'),('3034','','default','Tracking user activity','','7.13-dev'),('3035','','default','By enabling blocks such as <em>Who''s online</em> and <em>Who''s new</em>, site users can track who is logged in and new user signups at a centralized location.','','7.13-dev'),('3036','','default','Tracking content activity','','7.13-dev'),('3037','','default','By enabling blocks such as <em>Recent blog posts</em>, <em>New forum topics</em> and <em>Recent comments</em>, site users can view newly added site content at a glance.','','7.13-dev'),('3038','','default','View the administrative dashboard','','7.13-dev'),('3039','','default','This dashboard region is empty. Click <em>Customize dashboard</em> to add blocks to it.','','none'),('3040','','default','DRAG HERE','','none'),('3041','','default','Provides a dashboard page in the administrative interface for organizing administrative tasks and tracking information within your site.','','7.13-dev'),('3042','','default','Customize dashboard','','7.13-dev'),('3043','','default','Clear log messages','','none'),('3044','','default','This will permanently remove the log messages from the database.','','none');
INSERT INTO `locales_source` VALUES ('3045','','default','Database log cleared.','','none'),('3046','','default','The Database logging module logs system events in the Drupal database. For more information, see the online handbook entry for the <a href="@dblog">Database logging module</a>.','','7.13-dev'),('3047','','default','Monitoring your site','','7.13-dev'),('3048','','default','Debugging site problems','','7.13-dev'),('3049','','default','Field validation errors','','none'),('3050','','default','Attempt to create a field with a name longer than 32 characters: %name','','none'),('3051','','default','Attempt to create field name %name which already exists and is active.','','none'),('3052','','default','Attempt to create field name %name which already exists, although it is inactive.','','none'),('3053','','default','Attempt to create field name %name which is reserved by entity type %type.','','none'),('3054','','default','Attempt to create a field of unknown type %type.','','none'),('3055','','default','Attempt to create a field with unknown storage type %type.','','none'),('3056','','default','Attempt to create an instance of field @field_name without a bundle.','','none'),('3057','','default','Enabling field types','','7.13-dev'),('3058','','default','The Field module provides the infrastructure for fields and field attachment; the field types and input widgets themselves are provided by additional modules. Some of the modules are required; the optional modules can be enabled from the <a href="@modules">Modules administration page</a>. Drupal core includes the following field type modules: Number (required), Text (required), List (required), Taxonomy (optional), Image (optional), and File (optional); the required Options module provides input widgets for other field modules. Additional fields and widgets may be provided by contributed modules, which you can find in the <a href="@contrib">contributed module section of Drupal.org</a>. Currently enabled field and input widget modules:','','7.13-dev'),('3059','','default','Managing field data storage','','7.13-dev');
INSERT INTO `locales_source` VALUES ('3060','','default','Developers of field modules can either use the default <a href="@sql-store">Field SQL storage module</a> to store data for their fields, or a contributed or custom module developed using the <a href="@storage-api">field storage API</a>.','','7.13-dev'),('3061','','default','The Field SQL storage module stores field data in the database. It is the default field storage module; other field storage mechanisms may be available as contributed modules. See the <a href="@field-help">Field module help page</a> for more information about fields.','','7.13-dev'),('3062','','default','Default SQL storage','','7.13-dev'),('3063','','default','Stores fields in the local SQL database, using per-field tables.','','7.13-dev'),('3064','','default','Field SQL storage','','none'),('3065','','default','Stores field data in an SQL database.','','7.13-dev'),('3066','','default','The List module defines various fields for storing a list of items, for use with the Field module. Usually these items are entered through a select list, checkboxes, or radio buttons. See the <a href="@field-help">Field module help page</a> for more information about fields.','','7.13-dev'),('3067','','default','This field stores simple on/off or yes/no options.','','7.13-dev'),('3068','','default','List (text)','','7.13-dev'),('3069','','default','The ''checkboxes/radio buttons'' widget will display checkboxes if the <em>Number of values</em> option is greater than 1 for this field, otherwise radios will be displayed.','','none'),('3070','','default','Allowed HTML tags in labels: @tags','','none'),('3071','','default','The value of this field is being determined by the %function function and may not be changed.','','none'),('3072','','default','Allowed values list: each key must be a valid integer or decimal.','','none'),('3073','','default','Allowed values list: each key must be a string at most 255 characters long.','','none'),('3074','','default','Allo